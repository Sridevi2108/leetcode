class Solution:
    def firstUniqChar(self, s: str) -> int:
        char_count={}
        for ch in s:
            if ch in char_count:
                char_count[ch]+=1
            else:
                char_count[ch]=1
        for i,ch in enumerate(s):
            if char_count[ch]==1:
                return i
        return -1Â©leetcode
